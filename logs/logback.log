<script language="javascript">

    var httpObj;
    var timerID;

    jQuery(document).ready(function () {
        loadText();
        setInterval("loadText()", 1000 * 60); // 지수 reload time 60초
    })

    function loadText() {
        // var url = '/tfcommon/jisu/jisuData.json';	//로컬
        // var url = '/Flash_Data/jisuData.json';	//운영
             																					// 로컬									// 운영
        var url = window.location.hostname.indexOf('alpha') > -1 ? '/tfcommon/jisu/jisuData.json' : '/Flash_Data/jisuData.json';

        jQuery.ajax({
            type: "POST",
            url: url + "?cache=" + (new Date()).getTime(),
            dataType: "json",
            cache: false,
            success: function (data) {
                drawModal(data)
                drawCarousel(data)
            },
            complete: function (data) {

            },
            error: function (xhr, status, error) {
                console.log("에러발생:", error, status, xhr);
            }
        });
    }

    // INTER_SHRN_ISCD 코드를 한국어로 교체
    function codeToKorean(INTER_SHRN_ISCD) {
        switch(INTER_SHRN_ISCD) {
            case "U_0001": return "KOSPI";
            case "U_1001": return "KOSDAQ";
            case "N_.DJI": return "DOW";
            case "N_COMP": return "NASDAQ";
            case "N_SPX": return "S&P500";
            case "N_JP#NI225": return "니케이";
            case "N_SHANG": return "상해종합";
            case "N_HSCE": return "홍콩H";
            case "X_FX@KRW": return "원/달러";
            case "X_FX@CNY": return "위엔/달러";
            case "X_FX@JPY": return "엔/달러";
            case "I_Y0202": return "미국 10년T-NOTE";
            case "I_Y0103": return "회사채3년BBB-";
            case "I_Y0112": return "CD(91일)";
            case "I_Y0113": return "CP(91일)";
            case "I_Y0101": return "국고채3년";
            case "I_Y0106": return "국고채10년";
            case "I_Y0107": return "국민주택1종(5년)";
            case "I_Y0111": return "산금채(1년)";
            case "I_Y0114": return "콜(1일)";
            default:
                return ""
        }
    }

    

    function getUpDown(PRDY_VRSS_SIGN) {
        if (PRDY_VRSS_SIGN == '2') {
            return 'up'
        } else if (PRDY_VRSS_SIGN == '5') {
            return 'down'
        } else if (PRDY_VRSS_SIGN == '3')
            return 'zero'
        return ''
    }

    function getUpDownKor(PRDY_VRSS_SIGN) {
        if (PRDY_VRSS_SIGN == '2') {
            return '상승'
        } else if (PRDY_VRSS_SIGN == '5') {
            return '하락'
        } else if (PRDY_VRSS_SIGN == '3') {
            return '변동없음'
        }
    }

    function getPlusMinus(NTBY_TR_PBMN) {
        if (NTBY_TR_PBMN < 0) {
            return 'minus'
        }
        return 'plus'
    }

    function getUpDownByIncludeMinus(NTBY_TR_PBMN) {
        if (NTBY_TR_PBMN < 0) {
            return 'down'
        }
        return 'up'
    }

    function removeMinusSign(INTER_PRDY_VRSS) {
        return INTER_PRDY_VRSS.replace('-', '')
    }

    function drawCarouselDlElement(data, each) {
        var output = ''
        var _data = data[each]
        var loginId = 'null';
        var loginChk = true;
        if (loginId == 'null' || loginId == '') {
        	loginChk = false;
		}
        if(_data) {
            if ('N_SHANG' != each || loginChk) {

	        	output += '<dl>'
	            output += '<dt>'
	            output += codeToKorean(each)
	            output += '</dt>'
	            output += '<dd><span class="blind">현재가 </span><span>'
	            output += _data.INTER_PRPR
	            output += '</span></dd>'

	            var sign = getUpDown(_data.PRDY_VRSS_SIGN)
	            var signKor = getUpDownKor(_data.PRDY_VRSS_SIGN)

	            output += '<dd class="' + sign + '"><span class="blind">전일대비 </span><span class="arrow"><em>' + signKor + '</em>' + removeMinusSign(_data.INTER_PRDY_VRSS) + '</span></dd>'
	            output += '<dd class="' + sign + '"><span class="blind">등락률 </span><span class="arrow"><em>' + signKor + '</em>' + removeMinusSign(_data.PRDY_CTRT) + '</span></dd>'
	            output += '</dl>'
            }
        }

        return output
    }

    function drawCarousel(data) {
        var arr = ['U_0001', 'U_1001', 'N_.DJI', 'N_COMP', 'N_SPX', 'N_JP#NI225', 'N_SHANG', 'N_HSCE', 'X_FX@KRW', 'X_FX@CNY']
        if(!jQuery.trim(jQuery('.jisu_slide').html())) {
            var output = ''
            output += '<div class="jisu_track">'

           	var loginId = 'null';
            var loginChk = true;
            if (loginId == 'null' || loginId == '') {
            	loginChk = false;
    		}

            arr.forEach(function(each) {

            	if ('N_SHANG' != each || loginChk) {
	            	output += '<a href="javascript:void(0)" class="item" onclick="openJisuModal();return false;" data-name="' + each + '">'
	                output += drawCarouselDlElement(data, each)
	                output += '</a>'
            	}

            })
            output += '</div>';

            jQuery(".jisu_slide").html(output);
            jQuery(".jisu_slide").append(output);
        } else {
            arr.forEach(function(each) {
                var output = drawCarouselDlElement(data, each)
                var $target = jQuery('.jisu_track a[data-name="' + each + '"]')

                $target.each(function(){
                    jQuery(this).html(output)
                })
            })

        }

        try {
            if(jQuery('.jisu_con').data('load') === true) {
                setTimeout(function(){
                    UIcommon.jisu.refresh();
                }, 600);
            } else {
                setTimeout(function(){
                    UIcommon.jisu.init();
                }, 600);
            }
        } catch (e) {
        }
    }

    function numberWithComma(x) {
        return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g,',')
    }

    function drawModal(data) {
        var output = ''
        output += '<div class="modal_header">'
        output += '<div class="wrapper">'
        output += '<h2 class="title">지수종합</h2>'
        output += '<p><span>' + data.timestamp + '</span>최근 업데이트</p>'
        output += '</div>'
        output += '</div>'
        output += '<div class="modal_body">'
        output += '<div class="wrapper">'
        output += '<div class="comp ">'
        output += '<div class="storkmarket_area clearfix">'
        var kospi = data['U_0001']

        var sign = getUpDown(kospi.PRDY_VRSS_SIGN)
        var signText = "";
		if(sign == "up"){
			signText = "상승";
		}else if (sign == "zero"){
			signText = "변동없음";
		}else if(sign == "down"){
			signText = "하락";
		}
        output += '<div class="kospi ' + sign + '_area">';
        output += '<h3 class=\"mt_x0\">KOSPI</h3>';
        output += '<p class="stork_index">';
        output += '<strong>' + kospi.INTER_PRPR + '</strong>';
        output += '<span class="' + sign + '"><em class="blind">' + signText + '</em>' + removeMinusSign(kospi.INTER_PRDY_VRSS) + '</span>';
        output += '<span class="' + sign + '"><em class="blind">' + signText + '</em>' + removeMinusSign(kospi.PRDY_CTRT) + '%</span>';
        output += '</p>';
        output += '<ul class="trading_type clearfix">';

        sign = getUpDownByIncludeMinus(kospi.PRSN_NTBY_TR_PBMN)
        var plusMinus = getPlusMinus(kospi.PRSN_NTBY_TR_PBMN)
        var plusMinusSign = plusMinus === 'up' ? '+' : ''
        output += '<li>개인<em class="' + sign + '">' + plusMinusSign + numberWithComma(kospi.PRSN_NTBY_TR_PBMN) + '</em><span>억</span></li>';

        sign = getUpDownByIncludeMinus(kospi.FRGN_NTBY_TR_PBMN)
        plusMinus = getPlusMinus(kospi.FRGN_NTBY_TR_PBMN)
        plusMinusSign = plusMinus === 'up' ? '+' : ''
        output += '<li>외국인<em class="' + sign + '">' + plusMinusSign + numberWithComma(kospi.FRGN_NTBY_TR_PBMN) + '</em><span>억</span></li>';

        sign = getUpDownByIncludeMinus(kospi.ORGN_NTBY_TR_PBMN)
        plusMinus = getPlusMinus(kospi.ORGN_NTBY_TR_PBMN)
        plusMinusSign = plusMinus === 'up' ? '+' : ''
        output += '<li>기관<em class="' + sign + '">' + plusMinusSign + numberWithComma(kospi.ORGN_NTBY_TR_PBMN) + '</em><span>억</span></li>';

        output += '</ul>'
        output += '</div>'

