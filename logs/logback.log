			<script type="text/javascript">
				(function($) {
					$(document).ready(function(){
						
						var bookmarkArr = localStorage.getItem("homepageBookMarkData") == null || localStorage.getItem("homepageBookMarkData") == "" ? [] : localStorage.getItem("homepageBookMarkData").split("|");

						
						if(bookmarkArr.length > 0) {
							for(var i = 0; i < bookmarkArr.length; i++) {
								if(bookmarkArr[i] == "A_FP_20961732") {
									$('#btn-bookmark').addClass("on");
									break;
								}
							}
						}

						//BreadCrumbs Ajax 호출
						var $lis = $('.breadscrumb_wrap').find('.scroll'),
							mids = 'mids=';
						$lis.prev('A').each(function(i) {
							if(i>0) mids += ',';
							mids += $(this).data('mid');
						})

						$.ajax({
					    	url:'/templets/truefriend/breadCrumbs_2021.jsp',
					    	data : mids,
					    	cache : false,
					    	async : false,
					    	success:function(data){
								var $dom = $(data);

					    		$lis.each(function(i){
					    			$(this).append($dom.eq(i));
					    		});

					    		$dom.find('[data-pensionLevel]').each(function(i) {
					    			if(setPensionAnchor && userPensionLevel) {
						    			setPensionAnchor(this);
						    		} /* else {
						    			$(this).attr('href', 'javascript:g(\'/pension/nwMyinfo/BaseInfo.jsp?cmd=A_NW_30340\', \'L\');');
						    		} */
					    		});
					    	}
						});

						
						if(localStorage.getItem("homepageBookMarkData") != null && localStorage.getItem("homepageBookMarkData") != "") {
							xSubmitAjax(null, {
								url : '/templets/truefriend/getRecentInfo_2021.jsp?cmd=getBookMarkListInfo&bookMark=' + localStorage.getItem("homepageBookMarkData"),
								dataType : 'text',
								loadingBar : false,
						    	success:function(data){
					    			$('.bookMarkViewListArea').empty();
					    			$('.bookMarkViewListArea').append(data);
					    		}
							});
						}

						
						$('#btn-bookmark').on('click', function() {
							var bookMarkTypeCheck;
							if($('#btn-bookmark').hasClass("on")) {
								
								var bookCheckDelFlg = false;
								var bookCheckDelCnt;
								
								if(bookmarkArr.length > 0) {
									for(var i = 0; i < bookmarkArr.length; i++) {
										if(bookmarkArr[i] == "A_FP_20961732") {
											bookCheckDelFlg = true;
											bookCheckDelCnt = i;
											break;
										}
									}

									
									if(bookCheckDelFlg) {
										bookmarkArr.splice(bookCheckDelCnt, 1);
									}
									$('#btn-bookmark').removeClass("on");
									bookMarkTypeCheck = false;
								}
							}
							else {
								
								if(bookmarkArr.length < 9) {
									
									var bookCheckSucFlg = true;

									if(bookmarkArr.length > 0) {
										for(var i = 0; i < bookmarkArr.length; i++) {
											if(bookmarkArr[i] == "A_FP_20961732") {
												bookCheckSucFlg = false;
												break;
											}
										}
										
										if(bookCheckSucFlg) {
											bookmarkArr.push("A_FP_20961732");
										}
									}
									else {
										bookmarkArr.push("A_FP_20961732");
									}
									$('#btn-bookmark').addClass("on");
									bookMarkTypeCheck = true;
								}
								else {
									newAlert({
										msg : "즐겨찾기는 최대 9개까지 등록 가능합니다."
									})
									return;
								}
							}

							localStorage.setItem("homepageBookMarkData", bookmarkArr.join("|"));

							xSubmitAjax(null, {
								url : '/templets/truefriend/getRecentInfo_2021.jsp?cmd=getBookMarkListInfo&bookMark=' + localStorage.getItem("homepageBookMarkData"),
								dataType : 'text',
								loadingBar : false,
						    	success:function(data){
						    		
						    		if(data.trim() == "") {
							    		$('.bookMarkViewListNotArea').show();
							    		$('.bookMarkViewListArea').hide();
							    		$('.bookMarkDeleteBtn').hide();
						    		}
						    		else {
							    		$('.bookMarkViewListNotArea').hide();
							    		$('.bookMarkViewListArea').show();
							    		$('.bookMarkDeleteBtn').show();
						    		}

						    		if(bookMarkTypeCheck == true) {
						    			newAlert({
							    			msg : "즐겨찾기에 추가되었습니다."
							    		})
						    		}
						    		else {
						    			newAlert({
							    			msg : "즐겨찾기가 삭제되었습니다."
							    		})
						    		}
						    		$('.bookMarkViewListArea').empty();
						    		$('.bookMarkViewListArea').append(data);
						    	}
							});
						});

						
						$(document).on("click", '.bookmark-private-del-btn', function(e) {
							var dataId = e.currentTarget.dataset.id;

							var bookCheckPrivateFlg = false;
							var bookCheckPrivateCnt;

							
							if(bookmarkArr.length > 0) {
								for(var i = 0; i < bookmarkArr.length; i++) {
									if(bookmarkArr[i] == dataId) {
										bookCheckPrivateFlg = true;
										bookCheckPrivateCnt = i;
										break;
									}
								}

								
								if(bookCheckPrivateFlg) {
									bookmarkArr.splice(bookCheckPrivateCnt, 1);
								}

								localStorage.setItem("homepageBookMarkData", bookmarkArr.join("|"));
								xSubmitAjax(null, {
									url : '/templets/truefriend/getRecentInfo_2021.jsp?cmd=getBookMarkListInfo&bookMark=' + localStorage.getItem("homepageBookMarkData"),
									dataType : 'text',
									loadingBar : false,
							    	success : function(data){
							    		
							    		if(data.trim() == "") {
								    		$('.bookMarkViewListNotArea').show();
								    		$('.bookMarkViewListArea').hide();
								    		$('.bookMarkDeleteBtn').hide();
							    		}
							    		else {
								    		$('.bookMarkViewListNotArea').hide();
								    		$('.bookMarkViewListArea').show();
								    		$('.bookMarkDeleteBtn').show();
							    		}
							    		$('#btn-bookmark').removeClass("on");
							    		$('.bookMarkViewListArea').empty();
							    		$('.bookMarkViewListArea').append(data);
						    		}
								});
							}
						});

						
						$(document).on('click', '.bookMarkDeleteBtn', function() {
							if(confirm("즐겨찾기로 설정한 메뉴를 모두 삭제하시겠습니까?")) {
								
								
								if(bookmarkArr.length > 0) {
									bookmarkArr = [];
									localStorage.setItem("homepageBookMarkData", bookmarkArr.join("|"));

									xSubmitAjax(null, {
										url : '/templets/truefriend/getRecentInfo_2021.jsp?cmd=getBookMarkListInfo&bookMark=' + localStorage.getItem("homepageBookMarkData"),
										dataType : 'text',
										loadingBar : false,
								    	success:function(data){
								    		$('.bookMarkViewListNotArea').show();
								    		$('.bookMarkViewListArea').hide();
								    		$('.bookMarkDeleteBtn').hide();
								    		$('#btn-bookmark').removeClass("on");
								    		$('.bookMarkViewListArea').empty();
								    		$('.bookMarkViewListArea').append(data);
							    		}
									});
								}
							}
			    		});
					});
				})(jQuery)
