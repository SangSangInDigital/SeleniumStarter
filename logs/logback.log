		function isValidSMS(phoneNumberWithHyphen) {
		    if(/^[0-9]{2,3}-[0-9]{3,4}-[0-9]{4}/.test(phoneNumberWithHyphen)) {
		        return true
		    }
		    return false
		}

		function sendSMS() {
		    var phoneNumber = jQuery('input[name=phoneNumber]').val()
		    var phoneNumberWithHyphen = phoneNumber.replace(/(^02.{0}|^01.{1}|[0-9]{3})([0-9]+)([0-9]{4})/,"$1-$2-$3")
		    if(!isValidSMS(phoneNumberWithHyphen)) {
		        alert('휴대폰 번호를 입력해 주세요.')
		        return false
		    }

		    var phoneNumberDevided = phoneNumberWithHyphen.split('-')

		    jQuery.ajax({
		        type:"POST",
		        url : "/event/event_bankissms.jsp?cmd=sendSMS",
		        data: {
		            cs_phone1 : phoneNumberDevided[0],
		            cs_phone2 : phoneNumberDevided[1],
		            cs_phone3 : phoneNumberDevided[2]
		        },
		        dataType:"json",
		        cache: false,
		        success : function (data) {// 서버로 값을 성공적으로 넘기면 처리하는 코드부분 입니다.

		            if(data.h_rt_cd=='0'){
		                alert("SMS발송이 정상적으로 완료되었습니다.");
		            }else{
		                alert(data.h_msg1);
		            }
		        },
		        complete : function(data) {
		            // fn_init();
		        },
		        error : function(xhr, status, error) {
		            alert("에러발생:"+error +"\n"+status+"\n"+xhr+"\n"+xhr.responseText);
		        }
		    }); //end ajax
		}
	</script>
