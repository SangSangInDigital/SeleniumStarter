		            }else{
		                alert(data.h_msg1);
		            }
		        },
		        complete : function(data) {
		            // fn_init();
		        },
		        error : function(xhr, status, error) {
		            alert("에러발생:"+error +"\n"+status+"\n"+xhr+"\n"+xhr.responseText);
		        }
		    }); //end ajax
		}
